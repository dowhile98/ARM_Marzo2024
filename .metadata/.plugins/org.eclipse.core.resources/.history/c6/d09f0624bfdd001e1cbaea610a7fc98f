/**
 * PD12: Led
 *
 * PA5->LED
 * PC13 -> Boton de usuario
 */
/*Includes -------------------------------------------------------------------*/
#include "stm32f4xx.h"
#include "hvac_ll_driver.h"
/*Defines --------------------------------------------------------------------*/

/**
 * PD14
 * bit_word_addr = bit_band_base + (byte_offset x 32) + (bit_number Ã— 4)+
 * byte_offset = 0x20C00
 * bit_number = 12
 * bit_band_base = 0x42000000
 * LED_ALIAS = 0x42000000 + 0x20C00*0x20 + 0xC * 4
 */
#define LED  *((volatile uint8_t*)(0x42000000 + 0x20C00*0x20 + 0xC * 4))
