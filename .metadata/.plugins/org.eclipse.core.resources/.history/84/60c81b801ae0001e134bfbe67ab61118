/**
 * PD12: Led
 *
 * PA5->LED
 * PC13 -> Boton de usuario
 */
/*Includes -------------------------------------------------------------------*/
#include "stm32f4xx.h"
#include "hvac_ll_driver.h"
#include "delay.h"
#include "button.h"
/*Defines --------------------------------------------------------------------*/


/*Typedefs -------------------------------------------------------------------*/

/*Global variables -----------------------------------------------------------*/
Button_t sw1;
Button_t sw2;
/*Function prototype ---------------------------------------------------------*/
uint8_t readButton(uint8_t pin);


int main(void)
{
	/*Local variables --------------------------------------------------------*/

	/*
	 * base tiempo
	 * ticks = Fsysclk/freq
	 * ticks = 16E+6/1000
	 * ticks = 16000
	 */
	SystemCoreClockUpdate();
	SysTick_Config(SystemCoreClock / 1000);
	/**
	 * hardware init
	 */
	hvac_io_init();
	/**
	 * button
	 */

	/* Loop forever */
	for(;;){
		GPIOX_TOGGLE(STATUS);
		delay_ms(100);
	}
}

